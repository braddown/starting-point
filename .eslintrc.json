{
  "extends": "next/core-web-vitals",
  "rules": {
    "@typescript-eslint/no-unused-vars": "error",
    "@typescript-eslint/no-explicit-any": "error",
    "react/no-unescaped-entities": [
      "error",
      {
        "forbid": [
          {
            "char": "'",
            "alternatives": ["&apos;"]
          },
          {
            "char": "\"",
            "alternatives": ["&quot;"]
          }
        ]
      }
    ]
  }
} 